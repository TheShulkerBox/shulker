from ../models import Map

INSTANCE_STATE = Scoreboard("duels.instance_state")


class DuelsMap(Map):
    gamemode: str = "duels"

    location: tuple[int, int, int]
    spawn_1: tuple[int, int, int]
    spawn_2: tuple[int, int, int]
    dimension: str = "overworld"
    max_player_count: int = 2


macro duels_map path=brigadier:string data=mecha:nested_json:
    DuelsMap.generate(id=path, data=data)
