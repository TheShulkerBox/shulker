from ../models import Map, Team, MapTag

map = Map(
    id="dwarven",
    tags=[
        MapTag.Competitive,
        MapTag.CTF,
        MapTag.All,
    ],
    teams=[
        Team(id="smalls", color="red", spawn=(0, 0, 0, 0, 0)),
        Team(id="bigs", color="blue", spawn=(0, 0, 0, 0, 0)),
    ],
    location=(0, 65, 0),
    dimension="overworld",
)


function map.init:
    # Teleport players to the center of the map
    tp @a 0 65 0 -90 0
